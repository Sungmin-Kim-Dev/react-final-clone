{
  "routes": [{"src": "/[^.]+", "dest": "/", "status": 200}],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://www.kopis.or.kr/openApi/restful/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://www.kopis.or.kr/openApi/restful/:path*/"
    }
  ]
}
